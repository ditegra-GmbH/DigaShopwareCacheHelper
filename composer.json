{
  "name": "diga/shopwarecachehelper-plugin",
  "description": "shopwarecachehelper Plugin",
  "version": "3.0.1",
  "type": "shopware-platform-plugin",
  "license": "proprietary",
  "authors": [{
    "name": "ditegra GmbH"
  }],
  "autoload": {
    "psr-4": {
      "DigaShopwareCacheHelper\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "DigaShopwareCacheHelper\\DigaShopwareCacheHelper",
    "label": {
      "de-DE": "Cache Helper",
      "en-GB": "Cache Helper"
    },
    "copyright": "ditegra GmbH",
    "description": {
      "de-DE": "Hilft bei der Fehlersuche und Untersuchung, versteht des Shopware Caching, verschiedene Möglichkeiten, Cache-Ereignisse zu protokollieren und Shopware-Probleme zu überprüfen",
      "en-GB": "helps to debug and investigate, understand shopware caching, provides options to capture cache events, debug shopware issues. Creates a separate log file to capture cache events"
    },
    "manufacturerLink": {
      "de-DE": "https://ditegra.de",
      "en-GB": "https://ditegra.de"
    },
    "supportLink": {
      "de-DE": "https://ditegra.de",
      "en-GB": "https://ditegra.de"
    }
  },
  "require": {
    "shopware/core": "~6.7.1",
    "shopware/storefront": "~6.7.1"
  },
  "require-dev": {
      "phpunit/phpunit": "*",
      "phpstan/phpstan": "*",
      "friendsofphp/php-cs-fixer": "*",
    "rector/rector": "^2.1",
    "frosh/shopware-rector": "^0.5.6"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
